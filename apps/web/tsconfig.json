{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": [
    "tsconfig/base.json",
    "tsconfig/solid-js.json",
  ],
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": ["src/*"]
    },
    "types": [
      "vite/client",
      "vitest"
    ]
  }
}
