{
  "extends": ["//"],
  "pipeline": {
    "build": {
      "outputs": ["dist/**"]
    },
    "build:gh-pages": {
      "outputs": ["dist/**"]
    },
    "deploy:gh-pages": {
      "dependsOn": ["build:gh-pages"],
      "inputs": ["dist/**"]
    },
    "deploy:web": {
      "dependsOn": ["deploy:gh-pages"]
    }
  }
}
