{
  "name": "@price-prediction/web",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "lint": "ts-standard",
    "lint:fix": "ts-standard --fix",
