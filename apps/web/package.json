    "@suid/vite-plugin": "^0.1.4",
    "@testing-library/jest-dom": "^6.1.3",
    "gh-pages": "^3.2.3",
    "jsdom": "^22.1.0",
    "ts-standard": "^12.0.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-externalize-deps": "^0.7.0",
    "vite-plugin-solid": "^2.7.0",
