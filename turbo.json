{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {},
    "lint:fix": {},
    "check-types": {
      "outputs": ["tsconfig.tsbuildinfo"]
    },
    "test": {},
    "test:watch": {
      "cache": false,
      "persistent": true
    },
    "serve": {
      "dependsOn": ["build"]
    }
  }
}
