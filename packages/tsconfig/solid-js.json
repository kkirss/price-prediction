{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "solid-js",
  "extends": "./base.json",
  "exclude": ["node_modules", "dist"],
  "compilerOptions": {
    "incremental": true,
    "jsxImportSource": "solid-js",
    "jsx": "preserve",
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "esnext",
    "noEmit": true,
    "resolveJsonModule": true,
    "target": "esnext"
  }
}
